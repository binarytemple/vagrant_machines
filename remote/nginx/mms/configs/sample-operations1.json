{
    "pipelines": {
        "mduc": {
            "operations": [
                {
                    "operation": "resize",
                    "width": 1796,
                    "height": 1080
                },
                {
                    "operation": "text",
                    "fontFace": "Impact",
                    "colour": "red",
                    "size": 100,
                    "x1": 300,
                    "y1": 300,
                    "text": "${mduc}"
                }
            ],
            "output": "http://127.0.0.1:8089/pointofsale/${day}${month}${hour}${year}-${mduc}.jpg"
        },
        "amazon": {
            "operations": [
                {
                    "operation": "resize",
                    "width": 640,
                    "height": 980
                }
            ],
            "output": "http://127.0.0.1:8089/amazon/${day}${month}${hour}${year}.jpg"
        }
    }

}
